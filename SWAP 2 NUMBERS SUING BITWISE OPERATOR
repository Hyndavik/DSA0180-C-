#include <iostream>
using namespace std;

int main() {
    int a, b;

    cout << "Enter two numbers: ";
    cin >> a >> b;

    
    a = a ^ b;  
    b = a ^ b;  
    a = a ^ b;  

   
    cout << "After swapping: a = " << a << " and b = " << b << endl;

    return 0;
}

OUTPUT:
Enter two numbers: 5 10
After swapping: a = 10 and b = 5

